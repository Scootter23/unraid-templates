<?xml version="1.0"?>
<Container version="2">
  <Name>Cloudflare-Tunnel</Name>
  <Repository>cloudflare/cloudflared:latest</Repository>
  <Registry>https://hub.docker.com/r/cloudflare/cloudflared</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://github.com/cloudflare/cloudflared</Project>
  <Overview>Cloudflare Zero Trust Tunnel using the official docker from Cloudflared</Overview>
  <Category>Security:</Category>
  <WebUI/>
  <TemplateURL>https://raw.githubusercontent.com/Scootter23/unraid-templates/main/templates/cloudflare-tunnel.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/scootter23/unraid-templates/main/templates/img/cloudflare-zero-trust.png</Icon>
  <ExtraParams>--restart unless-stopped</ExtraParams>
  <PostArgs>tunnel --no-autoupdate run --token yourtoken</PostArgs>
  <CPUset/>
  <DonateText/>
  <DonateLink/>
  <Requires/>
</Container>
